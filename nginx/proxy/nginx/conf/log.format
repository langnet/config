log_format json '{"@timestamp":"$time_iso8601",' '"@source":"$server_addr",' '"@fields":{' '"client":"$remote_addr",' '"size":$body_bytes_sent,' '"responsetime":$request_time,' '"upstreamtime":$upstream_response_time,' '"oh":"$upstream_addr",' '"domain":"$host",' '"url":"$uri",' '"status":"$status"}}';   

log_format webserver '$remote_addr - $remote_user [$time_local] "$request"'
        '$status $body_bytes_sent "$http_referer"'
        '"$http_user_agent" $http_x_forwarded_for'
	'$request_time $host $upstream_addr $upstream_response_time' ;
